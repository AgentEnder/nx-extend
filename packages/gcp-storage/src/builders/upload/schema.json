{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "https://json-schema.org/draft-07/schema",
  "title": "upload",
  "description": "Upload files to Google Cloud Storage",
  "type": "object",
  "properties": {
    "bucket": {
      "type": "string",
      "description": "Bucket to upload to"
    },
    "directory": {
      "type": "string",
      "description": "Folder from the outputPath to upload",
      "default": "/"
    }
  }
}
